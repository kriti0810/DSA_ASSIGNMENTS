#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a,b,c;
    cout<<"Enter three numbers";
    cin>>a>>b>>c;
    int sum = 0;
    sum = a+b+c;
    cout<<"The sum of the three numbers is : "<<sum;   
    return 0;
}
