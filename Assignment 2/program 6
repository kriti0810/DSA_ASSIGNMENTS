 #include<bits/stdc++.h>
using namespace std;

bool isPrime(int a){
    return 1;
}
int main() {
    for(int i=1;i<30;i++){
         if (isPrime(i)){
            cout<<"the number "<<i<<" is prime\n";
         }
         else{
            cout<<"the number "<<i<<" is not prime\n";
          }
    }
}
